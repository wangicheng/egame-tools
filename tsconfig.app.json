{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "types": ["chrome"],
    "moduleResolution": "bundler",
    "module": "esnext",
    "skipLibCheck": true,
    "strict": true,
    "isolatedModules": true,
    "moduleDetection": "force"
  },
  "include": ["src"]
}
